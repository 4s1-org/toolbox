{
  "name": "@4s1/toolbox",
  "version": "4.0.1",
  "description": "Toolbox",
  "bugs": {
    "url": "https://gitlab.com/4s1/toolbox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/toolbox.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "type": "commonjs",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:dev": "pnpm run build -- --project tsconfig.dev.json",
    "start": "node dist/index.js",
    "start:dev": "node src/index.ts",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo No tests available",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "prepare": "husky install"
  },
  "prettier": "@4s1/eslint-config",
  "devDependencies": {
    "@4s1/eslint-config": "3.2.2",
    "@4s1/ts-config": "1.3.1",
    "@commitlint/cli": "15.0.0",
    "@commitlint/config-conventional": "15.0.0",
    "@types/node": "14.18.0",
    "eslint": "8.4.0",
    "husky": "7.0.4",
    "prettier": "2.5.0",
    "standard-version": "9.3.2",
    "typescript": "4.5.2"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/29537331/packages/npm/"
  }
}
