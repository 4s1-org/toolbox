{
  "name": "@4s1/toolbox",
  "version": "1.2.2",
  "description": "Toolbox",
  "bugs": {
    "url": "https://gitlab.com/4s1/toolbox/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/toolbox.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "test": "echo No tests available",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "prepare": "husky install"
  },
  "prettier": "@4s1/eslint-config",
  "devDependencies": {
    "@4s1/eslint-config": "3.1.2",
    "@4s1/semantic-release-config": "2.1.0",
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.1",
    "@semantic-release/gitlab": "6.2.2",
    "@types/node": "14.17.16",
    "conventional-changelog": "3.1.24",
    "conventional-changelog-conventionalcommits": "4.6.1",
    "eslint": "7.32.0",
    "husky": "7.0.2",
    "prettier": "2.4.0",
    "semantic-release": "17.4.7",
    "standard-version": "9.3.1",
    "typescript": "4.4.3"
  },
  "publishConfig": {
    "@4s1:registry": "https://gitlab.com/api/v4/projects/29537331/packages/npm/"
  }
}
